import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.ExpectedCondition;
import org.openqa.selenium.support.ui.WebDriverWait;
import org.testng.annotations.Test;
public class validEmail {
    @Test

    public void validEmail () {

  
System.setProperty("webdriver.chrome.driver");
WebDriver driver = new ChromeDriver();
driver.get("https://haveibeenpwned.com");
WebElement email = ((ChromeDriver) driver).findElement(By.id("Account"));
email.sendKeys("qwerty@somehting.com");
WebElement pwnedButton =driver.findElement(By.id("searchPwnage");
pwnedButton.click();
assert driver.findElement(By.id("Good news — no pwnage found!")).getText().startsWith("Good");

       
           
        
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.ExpectedCondition;
import org.openqa.selenium.support.ui.WebDriverWait;
import org.testng.annotations.Test;
public class invalidEmail {
    @Test

    public void validEmail () {

  
System.setProperty("webdriver.chrome.driver");
WebDriver driver = new ChromeDriver();
driver.get("https://haveibeenpwned.com");
WebElement email = ((ChromeDriver) driver).findElement(By.id("Account"));
email.sendKeys("test@something.com");
WebElement pwnedButton =driver.findElement(By.id("searchPwnage");
pwnedButton.click();
assert driver.findElement(By.id("Oh no — pwned!")).getText().startsWith("Oh");
